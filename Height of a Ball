import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int input1=sc.nextInt();
        int input2=sc.nextInt();
        int input3=sc.nextInt();
        int res=input2/input3;
        int res1=(res*res)*input1;
        System.out.println(res1);
    }
}
