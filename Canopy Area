import java.util.Scanner;

class Min {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int input1 = sc.nextInt();
        
        double a = 3.14 * input1 * input1;
        
        if (a -(int)a > 0.5) {
            System.out.println((int) a + 1); 
        } else {
            System.out.println((int) a); 
        }
    }
}
