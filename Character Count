public class CharacterCount {
    public static int countCharacter(String S, int N, char C) {
        int count = 0;
        for (int i = 0; i < N; i++) {
            if (S.charAt(i) == C) {
                count++;
            }
        }
        return count;
    }

    public static void main(String[] args) {
        String input1 = "hellowornd"; 
        int input2 = 10;
        char input3 = 'o'; 

        int result = countCharacter(input1, input2, input3);
        System.out.println(result); 
    }
}
